<%= render 'books/list', book: @book, user: @user%>
<h1>Users</h1>
<h4>image</h4>
<h4>name</h4>

<% @users.each do |user| %>
  <div>
    <%= link_to user_path(user.id) do %>
      <%= image_tag @book.get_image %>
    <% end %>
    
    <p><%= user.name %></p>
    <p><%= link_to "Show", user_path(user.id) %></p>
    <% end %>
    </div>